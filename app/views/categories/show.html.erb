<h1> <%= @category.name %></h1>


  <% @category.restaurants.order("created_at desc").each do |restaurant| %>
    <div>
      <%= link_to restaurant_path(restaurant) %>
      <%=restaurant.name%>
      <%= link_to "Book a table:",  new_restaurant_booking_path(restaurant) %>
    </div>
  <%end%>

</div>